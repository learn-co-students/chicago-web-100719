<h1>Hello world</h1>

<%= form_with model: @cat_dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <br>
  <%= f.label :rented_bed_ids %>
  <%= f.collection_check_boxes :rented_bed_ids, @pet_beds, :id, :description %>

  <%= f.submit %>
<% end %>
